    "body-parser": "^1.18.3",
    "cross-env": "^5.1.6",
    "express": "^4.16.3",
    "http": "0.0.0",
    "lodash": "^4.17.10",
    "mongoose": "^5.1.3",
    "morgan": "^1.9.0"
  },
  "devDependencies": {
    "@types/body-parser": "^1.17.0",
    "@types/express": "^4.11.1",
    "@types/mongodb": "^3.0.19",
    "@types/mongoose": "^5.0.15",
    "@types/morgan": "^1.7.35",
    "nodemon": "^1.17.5",
    "ts-node": "^6.1.0",
    "tslint": "^5.10.0",
    "typescript": "^2.9.1"
  }
