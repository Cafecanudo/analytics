  "description": "Eldoc Analytics - Sistema de Analise e monitoria de documentos fiscais ELDOC",
  "main": "loader.js",
  "scripts": {
    "lint": "./node_modules/.bin/tslint --project tsconfig.json ./server/**/*.ts",
    "start": "cross-env NODE_ENV=development nodemon --exec ./node_modules/.bin/ts-node ./server/server.ts",
    "build": "./node_modules/.bin/tsc --diagnostics",
