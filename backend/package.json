{
  "name": "Eldoc-Analytics",
  "version": "0.0.1-SNAPHOST",
  "description": "Eldoc Analytics - Sistema de Analise e monitoria de documentos fiscais ELDOC",
  "main": "./server/server.ts",
  "scripts": {
    "lint": "./node_modules/.bin/tslint --project tsconfig.json ./server/**/*.ts",
    "start-dev": "cross-env NODE_ENV=development nodemon ./node_modules/.bin/ts-node ./server/server.ts",
    "build": "./node_modules/.bin/tsc",
    "unit-test": "cross-env NODE_ENV=test mocha --opts ddist/tests/unit/config/mocha.opts ./dist/tests/unit/unit.test.js",
    "integration-test": "cross-env NODE_ENV=test mocha --opts ./tests/integration/config/mocha.opts",
    "integration-test-watch": "cross-env NODE_ENV=test mocha --opts ./tests/integration/config/mocha.opts --watch",
    "start": "npm run build && node ./dist/server/server.js",
    "builded": "npm run build && nodemon ./dist/server.ts"
  },
  "author": "by Wellton S. Barros - welltonbarros@softbox.com.br",
  "keywords": [
    "json",
    "node",
    "mongoose",
    "express",
    "bluebird",
    "restful",
    "passport",
    "typescript"
  ],
  "license": "ISC",
  "dependencies": {
    "body-parser": "^1.18.3",
    "colors": "^1.3.0",
    "compression": "^1.7.2",
    "cors": "^2.8.4",
    "cross-env": "^5.1.6",
    "express": "^4.16.3",
    "helmet": "^3.12.1",
    "http": "0.0.0",
    "lodash": "^4.17.10",
    "mongodb": "^3.0.10",
    "mongoose": "^5.1.3",
    "mongoose-unique-validator": "^2.0.1",
    "morgan": "^1.9.0",
    "reflect-metadata": "^0.1.12"
  },
  "devDependencies": {
    "@types/body-parser": "^1.17.0",
    "@types/chai": "^4.1.3",
    "@types/colors": "^1.2.1",
    "@types/cors": "^2.8.4",
    "@types/express": "^4.11.1",
    "@types/helmet": "0.0.38",
    "@types/mocha": "^5.2.1",
    "@types/mongodb": "^3.0.19",
    "@types/mongoose": "^5.0.15",
    "@types/mongoose-unique-validator": "^1.0.1",
    "@types/morgan": "^1.7.35",
    "@types/supertest": "^2.0.4",
    "chai": "^4.1.2",
    "mocha": "^5.2.0",
    "node-typescript": "^0.1.3",
    "nodemon": "^1.17.5",
    "pm2": "^2.10.4",
    "supertest": "^3.1.0",
    "testdouble": "^3.8.1",
    "ts-node": "^6.1.0",
    "tslint": "^5.10.0",
    "typescript": "^2.9.1"
  }
}
